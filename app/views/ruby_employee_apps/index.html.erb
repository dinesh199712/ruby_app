<p id="notice"><%= notice %></p>

<h1>Ruby Employee Apps</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Gender</th>
      <th>Designation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ruby_employee_apps.each do |ruby_employee_app| %>
      <tr>
        <td><%= ruby_employee_app.name %></td>
        <td><%= ruby_employee_app.age %></td>
        <td><%= ruby_employee_app.gender %></td>
        <td><%= ruby_employee_app.designation %></td>
        <td><%= link_to 'Show', ruby_employee_app %></td>
        <td><%= link_to 'Edit', edit_ruby_employee_app_path(ruby_employee_app) %></td>
        <td><%= link_to 'Destroy', ruby_employee_app, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ruby Employee App', new_ruby_employee_app_path %>
